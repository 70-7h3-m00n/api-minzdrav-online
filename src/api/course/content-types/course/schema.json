{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "typeCourse": {
      "type": "enumeration",
      "enum": [
        "Медицина",
        "Психология",
        "Диетология"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "durationTraining": {
      "type": "integer",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "categories": {
      "type": "component",
      "repeatable": true,
      "component": "shared.list-text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "programs": {
      "type": "component",
      "repeatable": true,
      "required": true,
      "component": "shared.list-text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "typeTraining": {
      "displayName": "typeTraining",
      "type": "component",
      "repeatable": true,
      "component": "shared.type-training",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subInfoContent": {
      "type": "component",
      "repeatable": true,
      "component": "shared.image-list",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subInfoText": {
      "type": "component",
      "repeatable": true,
      "component": "shared.list-text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "thisCourseFor": {
      "type": "component",
      "repeatable": true,
      "component": "shared.list-text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "imageCourse": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "startDateTraining": {
      "type": "string",
      "required": true,
      "default": "Каждое 5 и 20 число",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "studyingTime": {
      "type": "string",
      "required": true,
      "default": "От 500 часов",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "receivedDocuments": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "pathCourse": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "knowledgeList": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.list-text",
      "required": true
    },
    "skillList": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.list-text",
      "required": true
    },
    "listOfSkills": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.list-text",
      "required": true
    },
    "header": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "typeTrainingHeader": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "ourLicenses": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.image-list",
      "required": true
    },
    "priceCourse": {
      "displayName": "PriceCourse",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.price-course",
      "required": true
    },
    "trainingContent": {
      "displayName": "TrainingContent",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.training-content"
    },
    "script": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
